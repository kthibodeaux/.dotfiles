Backbone.View.extend({
  tagName: '${1:tag_name}',
  template: template('${2:template}'),

  initialize: function() {
    $0    
  },

  events: {

  },

  render: function() {
    var template = this.template( this.model.toJSON() );
    this.$el.html(template);
    return this;
  }
});